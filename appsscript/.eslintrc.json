{
  "env": {
    "browser": true,
    "node": true,
    "es2020": true
  },
  "extends": ["./node_modules/gts", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "@typescript-eslint/no-duplicate-enum-values": "off" // !! debug without
  },
  "ignorePatterns": ["template/**/*", "template-ui/**/*"]
}
